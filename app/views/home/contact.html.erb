<section class="slice sct-color-1">
    <div class="container">
        <div class="section-title section-title--style-1 text-center mb-2">
            <h3 class="section-title-inner text-normal">
              Écrivez-nous
          </h3>
          <span class="section-title-delimiter clearfix d-none"></span>
      </div>

      <span class="clearfix"></span>

      <div class="fluid-paragraph fluid-paragraph-sm c-gray-light strong-300 text-center">
       Vous pouvez nous envoyer un message en remplissant le formulaire suivant,
   </div>

   <span class="space-xs-xl"></span>

   <div class="row justify-content-center">
    <div class="col-lg-6">
        <!-- Contact form -->
       <%= form_tag("/", method: "get") do %>
        <form id="form_contact" data-action="../../php/send-email.php" class="form-default" role="form">
            <div class="form-group has-feedback">
                <%= text_field_tag 'nom', nil, placeholder:'Entrez votre nom', class: "form-control form-control-lg"  %>

                <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                <div class="help-block with-errors"></div>
            </div>

            <div class="form-group has-feedback">
             <%= email_field_tag 'email', nil, placeholder:'Entrez votre email', class: "form-control form-control-lg"  %>
                
                <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                <div class="help-block with-errors"></div>
            </div>

            <div class="form-group has-feedback">
                 <%= text_area_tag :description, nil, placeholder:'Message...', class: "form-control form-control-lg"  %>
                
                <span class="glyphicon form-control-feedback" aria-hidden="true"></span>

                <div class="help-block with-errors"></div>
            </div>

            <div class="">
                <button type="reset" class="btn-reset d-none"></button>
                 <%= submit_tag "Envoyer", class:"btn btn-styled btn-lg btn-block btn-base-1 mt-4" %>
                
            </div>
             <% end %>
        </form>
    </div>
</div>
</div>
</section>